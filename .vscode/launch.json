    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/script.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "sql",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/script.js",
            "args": ["--sql", "createView"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "load",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/script.js",
            "args": ["--load", "meta.zip"]
        }
    ]
}